{
	"hosting": [
		{
			"target": "prod",
			"public": "public",
			"ignore": [
				".DS_Store"
			]
		},
		{
			"target": "beta",
			"public": "public",
			"ignore": [
				".DS_Store"
			]
		},
		{
			"target": "discord",
			"redirects": [
				{
					"source": "/**",
					"destination": "https://discord.gg/BcXFAVKJZQ",
					"type": 301
				}
			]
		},
		{
			"target": "github",
			"redirects": [
				{
					"source": "/**",
					"destination": "https://github.com/Wixonic",
					"type": 301
				}
			]
		},
		{
			"target": "instagram",
			"redirects": [
				{
					"source": "/**",
					"destination": "https://www.instagram.com/wixonic12",
					"type": 301
				}
			]
		}
	],
	"emulators": {
		"hosting": {
			"port": 3000
		},
		"auth": {
			"port": 3001
		},
		"firestore": {
			"port": 3002
		},
		"functions": {
			"port": 3003
		},
		"storage": {
			"port": 3004
		},
		"database": {
			"port": 3005
		},
		"ui": {
			"enabled": true,
			"port": 8080
		},
		"singleProjectMode": true
	},
	"database": {
		"rules": "rules/database.json"
	},
	"firestore": {
		"rules": "rules/firestore.rules",
		"indexes": "rules/firestore.index"
	},
	"storage": {
		"rules": "rules/storage.rules"
	}
}