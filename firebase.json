{
	"hosting": [
		{
			"target": "root",
			"public": "./websites/root",
			"ignore": [
				".DS_Store"
			],
			"rewrites": [
				{
					"source": "**",
					"destination": "/lib/404.html"
				}
			],
			"headers": [
				{
					"source": "**",
					"headers": [
						{
							"key": "Access-Control-Allow-Origin",
							"value": "*"
						},
						{
							"key": "Access-Control-Allow-Methods",
							"value": "GET, OPTIONS"
						},
						{
							"key": "Cache-Control",
							"value": "max-age=7200, must-revalidate"
						}
					]
				}
			]
		},
		{
			"target": "accounts",
			"public": "./websites/accounts",
			"ignore": [
				".DS_Store"
			],
			"rewrites": [
				{
					"source": "**",
					"destination": "/lib/404.html"
				}
			],
			"headers": [
				{
					"source": "**",
					"headers": [
						{
							"key": "Access-Control-Allow-Origin",
							"value": "*"
						},
						{
							"key": "Access-Control-Allow-Methods",
							"value": "GET, OPTIONS"
						},
						{
							"key": "Cache-Control",
							"value": "max-age=7200, must-revalidate"
						}
					]
				}
			]
		},
		{
			"target": "admin",
			"public": "./websites/admin",
			"ignore": [
				".DS_Store"
			],
			"rewrites": [
				{
					"source": "**",
					"destination": "/lib/404.html"
				}
			],
			"headers": [
				{
					"source": "**",
					"headers": [
						{
							"key": "Access-Control-Allow-Origin",
							"value": "*"
						},
						{
							"key": "Access-Control-Allow-Methods",
							"value": "GET, OPTIONS"
						},
						{
							"key": "Cache-Control",
							"value": "max-age=7200, must-revalidate"
						}
					]
				}
			]
		},
		{
			"target": "assets",
			"public": "./websites/assets",
			"ignore": [
				".DS_Store"
			],
			"rewrites": [
				{
					"source": "**",
					"destination": "/lib/404.html"
				}
			],
			"headers": [
				{
					"source": "**",
					"headers": [
						{
							"key": "Access-Control-Allow-Origin",
							"value": "*"
						},
						{
							"key": "Access-Control-Allow-Methods",
							"value": "GET, OPTIONS"
						},
						{
							"key": "Cache-Control",
							"value": "max-age=43200"
						}
					]
				}
			]
		},
		{
			"target": "wixiLand",
			"public": "./websites/wixiLand",
			"ignore": [
				".DS_Store"
			],
			"rewrites": [
				{
					"source": "**",
					"destination": "/lib/404.html"
				}
			],
			"headers": [
				{
					"source": "**",
					"headers": [
						{
							"key": "Access-Control-Allow-Origin",
							"value": "*"
						},
						{
							"key": "Access-Control-Allow-Methods",
							"value": "GET, OPTIONS"
						},
						{
							"key": "Cache-Control",
							"value": "max-age=7200, must-revalidate"
						}
					]
				}
			]
		},
		{
			"target": "redirects",
			"redirects": [
				{
					"regex": "/discord(?:.*)",
					"destination": "https://discord.gg/BcXFAVKJZQ",
					"type": 301
				},
				{
					"regex": "/help(?:.*)",
					"destination": "https://discord.gg/tNfVPDmDdt",
					"type": 301
				},
				{
					"regex": "/github(?:.*)",
					"destination": "https://github.com/Wixonic",
					"type": 301
				},
				{
					"regex": "/instagram(?:.*)",
					"destination": "https://www.instagram.com/wixonic12",
					"type": 301
				},
				{
					"regex": "/youtube(?:.*)",
					"destination": "https://www.youtube.com/channel/UCnPN_SkWcIDNL-BjJGeDzQA",
					"type": 301
				},
				{
					"regex": "/twitch(?:.*)",
					"destination": "https://www.twitch.tv/wixonic",
					"type": 301
				},
				{
					"source": "**",
					"destination": "https://wixonic.fr",
					"type": 301
				}
			]
		},
		{
			"target": "functions",
			"rewrites": [
				{
					"source": "**",
					"function": {
						"functionId": "httpServer",
						"region": "europe-west1"
					}
				}
			]
		}
	],
	"emulators": {
		"singleProjectMode": true,
		"ui": {
			"enabled": true,
			"host": "localhost",
			"port": 2000
		},
		"auth": {
			"host": "localhost",
			"port": 2001
		},
		"firestore": {
			"host": "localhost",
			"port": 2002
		},
		"storage": {
			"host": "localhost",
			"port": 2003
		},
		"functions": {
			"host": "localhost",
			"port": 2004
		},
		"hosting": {
			"host": "localhost",
			"port": 2005
		}
	},
	"storage": {
		"rules": "rules/storage.rules"
	},
	"firestore": {
		"rules": "rules/firestore.rules",
		"indexes": "rules/firestore.indexes.json"
	},
	"functions": [
		{
			"source": "functions",
			"codebase": "default",
			"ignore": [
				"node_modules",
				".git",
				"firebase-debug.log",
				"firebase-debug.*.log"
			]
		}
	]
}