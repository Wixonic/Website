{
	"hosting": [
		{
			"target": "root",
			"public": "websites/root",
			"ignore": [
				".DS_Store"
			],
			"redirects": [
				{
					"regex": "/assets(.*)",
					"destination": "https://assets.wixonic.fr/:1",
					"type": 301
				},
				{
					"regex": "/go(.*)",
					"destination": "https://go.wixonic.fr/:1",
					"type": 301
				}
			],
			"rewrites": [
				{
					"source": "**",
					"destination": "/lib/404.html"
				}
			]
		},
		{
			"target": "accounts",
			"public": "websites/accounts",
			"ignore": [
				".DS_Store"
			],
			"redirects": [
				{
					"regex": "/assets(.*)",
					"destination": "https://assets.wixonic.fr/:1",
					"type": 301
				},
				{
					"regex": "/go(.*)",
					"destination": "https://go.wixonic.fr/:1",
					"type": 301
				}
			],
			"rewrites": [
				{
					"source": "**",
					"destination": "/lib/404.html"
				}
			]
		},
		{
			"target": "admin",
			"public": "websites/admin",
			"ignore": [
				".DS_Store"
			],
			"redirects": [
				{
					"regex": "/assets(.*)",
					"destination": "https://assets.wixonic.fr/:1",
					"type": 301
				},
				{
					"regex": "/go(.*)",
					"destination": "https://go.wixonic.fr/:1",
					"type": 301
				}
			],
			"rewrites": [
				{
					"source": "**",
					"destination": "/lib/404.html"
				}
			]
		},
		{
			"target": "admin",
			"public": "websites/admin",
			"ignore": [
				".DS_Store"
			],
			"redirects": [
				{
					"regex": "/assets(.*)",
					"destination": "https://assets.wixonic.fr/:1",
					"type": 301
				},
				{
					"regex": "/go(.*)",
					"destination": "https://go.wixonic.fr/:1",
					"type": 301
				}
			],
			"rewrites": [
				{
					"source": "**",
					"destination": "/lib/404.html"
				}
			]
		},
		{
			"target": "functions",
			"rewrites": [
				{
					"source": "**",
					"function": {
						"functionId": "httpServer",
						"region": "europe-west1"
					}
				}
			]
		}
	],
	"emulators": {
		"singleProjectMode": true,
		"ui": {
			"enabled": true,
			"host": "localhost",
			"port": 2000
		},
		"auth": {
			"host": "localhost",
			"port": 2001
		},
		"firestore": {
			"host": "localhost",
			"port": 2002
		},
		"storage": {
			"host": "localhost",
			"port": 2003
		},
		"functions": {
			"host": "localhost",
			"port": 2004
		},
		"hosting": {
			"host": "localhost",
			"port": 2005
		}
	},
	"storage": {
		"rules": "rules/storage.rules"
	},
	"firestore": {
		"rules": "rules/firestore.rules",
		"indexes": "rules/firestore.indexes.json"
	},
	"functions": [
		{
			"source": "functions",
			"codebase": "default",
			"ignore": [
				"node_modules",
				".git",
				"firebase-debug.log",
				"firebase-debug.*.log"
			]
		}
	]
}